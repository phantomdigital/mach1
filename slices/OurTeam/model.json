{
  "id": "our_team",
  "type": "SharedSlice",
  "name": "OurTeam",
  "description": "Team members showcase with photos and positions",
  "variations": [
    {
      "id": "default",
      "name": "Default",
      "docURL": "...",
      "version": "initial",
      "description": "Default",
      "imageUrl": "",
      "primary": {
        "heading": {
          "type": "Text",
          "config": {
            "label": "Heading",
            "placeholder": "Our leadership team."
          }
        },
        "subheading": {
          "type": "Text",
          "config": {
            "label": "Subheading",
            "placeholder": "TEAM"
          }
        },
        "description": {
          "type": "Text",
          "config": {
            "label": "Description",
            "placeholder": "Follow the steps below and we will get a quote to you as soon as possible."
          }
        },
        "enable_dialog": {
          "type": "Boolean",
          "config": {
            "label": "Enable Team Member Dialog",
            "placeholder_false": "Disabled",
            "placeholder_true": "Enabled",
            "default_value": false
          }
        }
      },
      "items": {
        "name": {
          "type": "Text",
          "config": {
            "label": "Name",
            "placeholder": "Cheyne Warren"
          }
        },
        "position": {
          "type": "Text",
          "config": {
            "label": "Position",
            "placeholder": "Founder"
          }
        },
        "department": {
          "type": "Select",
          "config": {
            "label": "Department/Category",
            "placeholder": "Select department",
            "options": [
              "Leadership",
              "Operations",
              "Sales & Business Development",
              "Customer Service",
              "Finance & Administration",
              "Technology & IT",
              "Warehouse & Distribution",
              "Transportation",
              "Human Resources",
              "Management",
              "Marketing",
              "Legal",
              "Customer Service"
            ]
          }
        },
        "image": {
          "type": "Image",
          "config": {
            "label": "Profile Image",
            "constraint": {
              "width": 400,
              "height": 400
            },
            "thumbnails": []
          }
        },
        "bio": {
          "type": "StructuredText",
          "config": {
            "label": "Biography",
            "placeholder": "Write a brief bio about this team member...",
            "single": "paragraph,strong,em,hyperlink"
          }
        },
        "email": {
          "type": "Text",
          "config": {
            "label": "Email (Optional)",
            "placeholder": "john@mach1logistics.com.au"
          }
        },
        "linkedin": {
          "type": "Link",
          "config": {
            "label": "LinkedIn Profile (Optional)",
            "placeholder": "LinkedIn URL"
          }
        }
      }
    }
  ]
}
